Cell[BoxData[{
 RowBox[{"ClearAll", "[", "VerifyCertificate", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"VerifyCertificate", "[", 
   RowBox[{"poly_", ",", 
    RowBox[{"{", 
     RowBox[{"lb_", ",", "ub_"}], "}"}], ",", "bound_", ",", "S_", ",", 
    "SWts_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "vars", ",", "n", ",", "d", ",", "degs", ",", "basis", ",", "degsWt", 
      ",", "basisWt", ",", "wts", ",", "certifiedWSOSpoly"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Min", "[", 
        RowBox[{
         RowBox[{"LDLT", "[", "S", "]"}], "\[LeftDoubleBracket]", 
         RowBox[{"-", "1"}], "\[RightDoubleBracket]"}], "]"}], "<", "0"}], 
      ",", 
      RowBox[{
       RowBox[{"Print", "[", "\"\<S is not positive semidefinite\>\"", "]"}], 
       ";", 
       RowBox[{"Return", "[", "False", "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"Or", "@@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"Min", "[", 
              RowBox[{
               RowBox[{"LDLT", "[", "#", "]"}], "\[LeftDoubleBracket]", 
               RowBox[{"-", "1"}], "\[RightDoubleBracket]"}], "]"}], "<", 
             "0"}], "&"}], "/@", "SWts"}], ")"}]}], ",", 
        RowBox[{
         RowBox[{
         "Print", "[", 
          "\"\<Some matrix from SWts is not positive semidefinite\>\"", "]"}],
          ";", 
         RowBox[{"Return", "[", "False", "]"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"vars", "=", 
          RowBox[{"Sort", "[", 
           RowBox[{"Variables", "[", "poly", "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"n", "=", 
          RowBox[{"Length", "[", "vars", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"d", "=", 
          RowBox[{"Ceiling", "[", 
           RowBox[{
            RowBox[{"PolynomialTotalDegree", "[", "poly", "]"}], "/", "2"}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"degs", "=", 
          RowBox[{"DegreeSequences", "[", 
           RowBox[{"n", ",", "d"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"basis", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"Product", "[", 
             RowBox[{
              RowBox[{"ChebyshevT", "[", 
               RowBox[{
                RowBox[{"degs", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", ",", "k"}], "]"}], "]"}], ",", 
                RowBox[{
                 RowBox[{"-", "1"}], "+", 
                 RowBox[{"2", 
                  FractionBox[
                   RowBox[{
                    RowBox[{"vars", "[", 
                    RowBox[{"[", "k", "]"}], "]"}], "-", 
                    RowBox[{"lb", "[", 
                    RowBox[{"[", "k", "]"}], "]"}]}], 
                   RowBox[{
                    RowBox[{"ub", "[", 
                    RowBox[{"[", "k", "]"}], "]"}], "-", 
                    RowBox[{"lb", "[", 
                    RowBox[{"[", "k", "]"}], "]"}]}]]}]}]}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"k", ",", "n"}], "}"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{"Binomial", "[", 
               RowBox[{
                RowBox[{"n", "+", "d"}], ",", "n"}], "]"}]}], "}"}]}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"degsWt", "=", 
          RowBox[{"DegreeSequences", "[", 
           RowBox[{"n", ",", 
            RowBox[{"d", "-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"basisWt", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"Product", "[", 
             RowBox[{
              RowBox[{"ChebyshevT", "[", 
               RowBox[{
                RowBox[{"degsWt", "[", 
                 RowBox[{"[", 
                  RowBox[{"i", ",", "k"}], "]"}], "]"}], ",", 
                RowBox[{
                 RowBox[{"-", "1"}], "+", 
                 RowBox[{"2", 
                  FractionBox[
                   RowBox[{
                    RowBox[{"vars", "[", 
                    RowBox[{"[", "k", "]"}], "]"}], "-", 
                    RowBox[{"lb", "[", 
                    RowBox[{"[", "k", "]"}], "]"}]}], 
                   RowBox[{
                    RowBox[{"ub", "[", 
                    RowBox[{"[", "k", "]"}], "]"}], "-", 
                    RowBox[{"lb", "[", 
                    RowBox[{"[", "k", "]"}], "]"}]}]]}]}]}], "]"}], ",", 
              RowBox[{"{", 
               RowBox[{"k", ",", "n"}], "}"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{"Binomial", "[", 
               RowBox[{
                RowBox[{"n", "+", "d", "-", "1"}], ",", "n"}], "]"}]}], 
             "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"wts", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{
               RowBox[{"ub", "[", 
                RowBox[{"[", "k", "]"}], "]"}], "-", 
               RowBox[{"vars", "[", 
                RowBox[{"[", "k", "]"}], "]"}]}], ")"}], 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"vars", "[", 
                RowBox[{"[", "k", "]"}], "]"}], "-", 
               RowBox[{"lb", "[", 
                RowBox[{"[", "k", "]"}], "]"}]}], ")"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"k", ",", "n"}], "}"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"certifiedWSOSpoly", "=", 
          RowBox[{
           RowBox[{"basis", ".", "S", ".", "basis"}], "+", 
           RowBox[{"Sum", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
