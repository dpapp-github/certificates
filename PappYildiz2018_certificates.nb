Cell[BoxData[{
 RowBox[{"ClearAll", "[", "GenerateCertificate", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"GenerateCertificate", "[", 
   RowBox[{"poly_", ",", 
    RowBox[{"{", 
     RowBox[{"lb_", ",", "ub_"}], "}"}], ",", "rawx_", ",", "rawPts_", ",", 
    "xprec_", ",", "bound_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "vars", ",", "n", ",", "d", ",", "x", ",", "pts", ",", "polyVals", ",", 
      "degs", ",", "P", ",", "PWt", ",", "gi", ",", "s", ",", "g", ",", "H", 
      ",", "w", ",", "gWt", ",", "HWt", ",", "Lambda", ",", "LambdaWts", ",", 
      "PLi", ",", "S", ",", "PLiWts", ",", "SWts"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"vars", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{"Variables", "[", "poly", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"n", "=", 
      RowBox[{"Length", "[", "vars", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"d", "=", 
      RowBox[{"Ceiling", "[", 
       RowBox[{
        RowBox[{"PolynomialTotalDegree", "[", "poly", "]"}], "/", "2"}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"x", "=", 
      RowBox[{"Rationalize", "[", 
       RowBox[{"rawx", ",", "xprec"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"pts", "=", 
      RowBox[{"Rationalize", "[", 
       RowBox[{"rawPts", ",", "0"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"polyVals", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"poly", "/.", 
         RowBox[{"Thread", "[", 
          RowBox[{"vars", "\[Rule]", "t"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "pts"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"degs", "=", 
      RowBox[{"DegreeSequences", "[", 
       RowBox[{"n", ",", "d"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"P", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Product", "[", 
         RowBox[{
          RowBox[{"ChebyshevT", "[", 
           RowBox[{
            RowBox[{"degs", "[", 
             RowBox[{"[", 
              RowBox[{"i", ",", "k"}], "]"}], "]"}], ",", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             RowBox[{"2", 
              FractionBox[
               RowBox[{
                RowBox[{"pts", "[", 
                 RowBox[{"[", 
                  RowBox[{"j", ",", "k"}], "]"}], "]"}], "-", 
                RowBox[{"lb", "[", 
                 RowBox[{"[", "k", "]"}], "]"}]}], 
               RowBox[{
                RowBox[{"ub", "[", 
                 RowBox[{"[", "k", "]"}], "]"}], "-", 
                RowBox[{"lb", "[", 
                 RowBox[{"[", "k", "]"}], "]"}]}]]}]}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", 
          RowBox[{"Length", "[", "pts", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Binomial", "[", 
           RowBox[{
            RowBox[{"n", "+", "d"}], ",", "n"}], "]"}]}], "}"}]}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"PWt", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Product", "[", 
         RowBox[{
          RowBox[{"ChebyshevT", "[", 
           RowBox[{
            RowBox[{"degs", "[", 
             RowBox[{"[", 
              RowBox[{"i", ",", "k"}], "]"}], "]"}], ",", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             RowBox[{"2", 
              FractionBox[
               RowBox[{
                RowBox[{"pts", "[", 
                 RowBox[{"[", 
                  RowBox[{"j", ",", "k"}], "]"}], "]"}], "-", 
                RowBox[{"lb", "[", 
                 RowBox[{"[", "k", "]"}], "]"}]}], 
               RowBox[{
                RowBox[{"ub", "[", 
                 RowBox[{"[", "k", "]"}], "]"}], "-", 
                RowBox[{"lb", "[", 
                 RowBox[{"[", "k", "]"}], "]"}]}]]}]}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "n"}], "}"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", 
          RowBox[{"Length", "[", "pts", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Binomial", "[", 
           RowBox[{
            RowBox[{"n", "+", "d", "-", "1"}], ",", "n"}], "]"}]}], "}"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"gi", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
           "ub", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "-", 
           RowBox[{
            RowBox[{
            "pts", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], 
            "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{
            "pts", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], 
            "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], "-", 
           RowBox[{
           "lb", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}], 
          ")"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "n"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", 
          RowBox[{"Length", "[", "pts", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"s", "=", 
      RowBox[{"polyVals", "-", "bound"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"g", ",", "H"}], "}"}], "=", 
      RowBox[{"gHSOSWt", "[", 
       RowBox[{"x", ",", "P", ",", 
        RowBox[{"Array", "[", 
         RowBox[{
          RowBox[{"1", "&"}], ",", 
          RowBox[{"Binomial", "[", 
           RowBox[{
            RowBox[{"n", "+", 
             RowBox[{"2", "d"}]}], ",", "n"}], "]"}]}], "]"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"gWt", ",", "HWt"}], "}"}], "=", 
         RowBox[{"gHSOSWt", "[", 
          RowBox[{"x", ",", "PWt", ",", 
           RowBox[{"gi", "[", 
            RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"g", "+=", "gWt"}], ";", "\[IndentingNewLine]", 
        RowBox[{"H", "+=", "HWt"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"Length", "[", "gi", "]"}]}], "}"}]}], "\[IndentingNewLine]",
       "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"w", "=", 
      RowBox[{"LinearSolve", "[", 
       RowBox[{"H", ",", "s"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Lambda", "=", 
      RowBox[{
       RowBox[{"P", "\[Transpose]"}], ".", 
       RowBox[{"DiagonalMatrix", "[", "x", "]"}], ".", "P"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"LambdaWts", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"PWt", "\[Transpose]"}], ".", 
         RowBox[{"DiagonalMatrix", "[", 
          RowBox[{
           RowBox[{
           "gi", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
           "x"}], "]"}], ".", "PWt"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "gi", "]"}]}], "}"}]}], "]"}]}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"PLi", "=", 
      RowBox[{"Transpose", "[", 
       RowBox[{"LinearSolve", "[", 
        RowBox[{"Lambda", ",", 
         SuperscriptBox["P", "\[Transpose]"]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"S", "=", 
      RowBox[{
       SuperscriptBox["PLi", "\[Transpose]"], ".", 
       RowBox[{"DiagonalMatrix", "[", "w", "]"}], ".", "PLi"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"PLiWts", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Transpose", "[", 
         RowBox[{"LinearSolve", "[", 
          RowBox[{
           RowBox[{
           "LambdaWts", "\[LeftDoubleBracket]", "i", 
            "\[RightDoubleBracket]"}], ",", 
           SuperscriptBox["PWt", "\[Transpose]"]}], "]"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "LambdaWts", "]"}]}], "}"}]}], "]"}]}], ";", 
     " ", "\[IndentingNewLine]", 
     RowBox[{"SWts", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         SuperscriptBox[
          RowBox[{
          "PLiWts", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
          "\[Transpose]"], ".", 
         RowBox[{"DiagonalMatrix", "[", 
          RowBox[{
           RowBox[{
           "gi", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
           "w"}], "]"}], ".", 
         RowBox[{
         "PLiWts", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}]}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", 
          RowBox[{"Length", "[", "LambdaWts", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"S", ",", "SWts"}], "}"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",
